#! /bin/sh
echo  "checking please wait (3mins) ....."
 find ../../src -name "*.cc" -exec python ../../src/cpplint.py verbose=1  {} \;   > code_style_errors 2>&1
 find ../../src -name "*.h" -exec python ../../src/cpplint.py  verbose=1  {} \;   >> code_style_errors 2>&1
wc -l code_style_errors

echo "do ya think thats good enough ? "
