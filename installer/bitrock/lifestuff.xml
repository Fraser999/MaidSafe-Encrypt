                        </distributionFile>
                        <distributionFile>
                            <origin>../../build/install_files/QtGui4.dll</origin>
                        </distributionFile>
                        <distributionDirectory>
                            <origin>../../build/install_files/dokan</origin>
                        </distributionDirectory>
                        <distributionFile>
