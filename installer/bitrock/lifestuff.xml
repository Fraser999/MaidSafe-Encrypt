<project>
    <shortName>pd</shortName>
    <fullName>Perpetual Data</fullName>
    <version>0.0.004</version>
    <readmeFile>${build_project_directory}/../text/pd/readme.txt</readmeFile>
    <licenseFile>${build_project_directory}/../text/pd/eula.txt</licenseFile>
    <preBuildActionList>
        <actionGroup>
            <explanation>Copy Dokan to install_files/</explanation>
            <actionList>
                <createDirectory>
                    <path>..\..\build\install_files\dokan\Windows 7</path>
                </createDirectory>
                <createDirectory>
                    <path>..\..\build\install_files\dokan\Windows Vista and Windows Server 2008</path>
                </createDirectory>
                <createDirectory>
                    <path>..\..\build\install_files\dokan\Windows XP</path>
                </createDirectory>
                <findFile>
                    <baseDirectory>${build_project_directory}\..\..\build\Win</baseDirectory>
                    <pattern>Dokan_*</pattern>
                    <variable>DOKAN_ROOT</variable>
                </findFile>
                <copyFile>
                    <destination>..\..\build\install_files\dokan\Windows 7\dokan.dll</destination>
                    <origin>${DOKAN_ROOT}\Windows 7\dokan.dll</origin>
                </copyFile>
                <copyFile>
                    <destination>..\..\build\install_files\dokan\Windows 7\dokan.sys</destination>
                    <origin>${DOKAN_ROOT}\Windows 7\dokan.sys</origin>
                </copyFile>
                <copyFile>
                    <destination>..\..\build\install_files\dokan\Windows 7\dokanctl.exe</destination>
                    <origin>${DOKAN_ROOT}\Windows 7\dokanctl.exe</origin>
                </copyFile>
                <copyFile>
                    <destination>..\..\build\install_files\dokan\Windows 7\mounter.exe</destination>
                    <origin>${DOKAN_ROOT}\Windows 7\mounter.exe</origin>
                </copyFile>
                <copyFile>
                    <destination>..\..\build\install_files\dokan\Windows Vista and Windows Server 2008\dokan.dll</destination>
                    <origin>${DOKAN_ROOT}\Windows Vista and Windows Server 2008\dokan.dll</origin>
                </copyFile>
                <copyFile>
                    <destination>..\..\build\install_files\dokan\Windows Vista and Windows Server 2008\dokan.sys</destination>
                    <origin>${DOKAN_ROOT}\Windows Vista and Windows Server 2008\dokan.sys</origin>
                </copyFile>
                <copyFile>
                    <destination>..\..\build\install_files\dokan\Windows Vista and Windows Server 2008\dokanctl.exe</destination>
                    <origin>${DOKAN_ROOT}\Windows Vista and Windows Server 2008\dokanctl.exe</origin>
                </copyFile>
                <copyFile>
                    <destination>..\..\build\install_files\dokan\Windows Vista and Windows Server 2008\mounter.exe</destination>
                    <origin>${DOKAN_ROOT}\Windows Vista and Windows Server 2008\mounter.exe</origin>
                </copyFile>
                <copyFile>
                    <destination>..\..\build\install_files\dokan\Windows XP\dokan.dll</destination>
                    <origin>${DOKAN_ROOT}\Windows XP\dokan.dll</origin>
                </copyFile>
                <copyFile>
                    <destination>..\..\build\install_files\dokan\Windows XP\dokan.sys</destination>
                    <origin>${DOKAN_ROOT}\Windows XP\dokan.sys</origin>
                </copyFile>
                <copyFile>
                    <destination>..\..\build\install_files\dokan\Windows XP\dokanctl.exe</destination>
                    <origin>${DOKAN_ROOT}\Windows XP\dokanctl.exe</origin>
                </copyFile>
                <copyFile>
                    <destination>..\..\build\install_files\dokan\Windows XP\mounter.exe</destination>
                    <origin>${DOKAN_ROOT}\Windows XP\mounter.exe</origin>
                </copyFile>
            </actionList>
        </actionGroup>
        <actionGroup>
            <explanation>Copy exes to install_files/</explanation>
            <actionList>
                <findFile>
                    <baseDirectory>${build_project_directory}\..\..\build\Win</baseDirectory>
                    <pattern>perpetualdata.exe</pattern>
                    <variable>PERPETUALDATA_EXE</variable>
                    <ruleList>
                        <platformTest>
                            <type>windows</type>
                        </platformTest>
                    </ruleList>
                </findFile>
                <findFile>
                    <baseDirectory>${build_project_directory}\..\..\build\Linux</baseDirectory>
                    <pattern>perpetualdata</pattern>
                    <variable>PERPETUALDATA_EXE</variable>
                    <ruleList>
                        <platformTest>
                            <negate>1</negate>
                            <type>windows</type>
                        </platformTest>
                    </ruleList>
                </findFile>
                <copyFile>
                    <destination>..\..\build\install_files</destination>
                    <origin>${PERPETUALDATA_EXE}</origin>
                </copyFile>
                <findFile>
                    <baseDirectory>${build_project_directory}\..\..\build\Win</baseDirectory>
                    <pattern>vault.exe</pattern>
                    <variable>VAULT_EXE</variable>
                    <ruleList>
                        <platformTest>
                            <type>windows</type>
                        </platformTest>
                    </ruleList>
                </findFile>
                <findFile>
                    <baseDirectory>${build_project_directory}\..\..\build\Linux</baseDirectory>
                    <pattern>vault</pattern>
                    <variable>VAULT_EXE</variable>
                    <ruleList>
                        <platformTest>
                            <negate>1</negate>
                            <type>windows</type>
                        </platformTest>
                    </ruleList>
                </findFile>
                <copyFile>
                    <destination>..\..\build\install_files</destination>
                    <origin>${VAULT_EXE}</origin>
                </copyFile>
                <findFile>
                    <baseDirectory>${build_project_directory}\..\..\build</baseDirectory>
                    <pattern>CMakeCache.txt</pattern>
                    <variable>CMAKECACHE_TXT</variable>
                </findFile>
                <propertiesFileGet>
                    <file>${CMAKECACHE_TXT}</file>
                    <key>QT_BINARY_DIR:INTERNAL</key>
                    <variable>QT_BINARY_DIR</variable>
                </propertiesFileGet>
                <copyFile>
                    <destination>..\..\build\install_files</destination>
                    <origin>${QT_BINARY_DIR}/QtCore4.dll</origin>
                </copyFile>
                <copyFile>
                    <destination>..\..\build\install_files</destination>
                    <origin>${QT_BINARY_DIR}/QtGui4.dll</origin>
                </copyFile>
                <propertiesFileGet>
                    <file>${CMAKECACHE_TXT}</file>
                    <key>INSTALLBUILDER_ROOT_DIR:INTERNAL</key>
                    <variable>INSTALLBUILDER_ROOT_DIR</variable>
                </propertiesFileGet>
                <findFile>
                    <baseDirectory>${INSTALLBUILDER_ROOT_DIR}/autoupdate/output</baseDirectory>
                    <pattern>autoupdate-*</pattern>
                    <variable>AUTOUPDATE_EXE</variable>
                </findFile>
                <copyFile>
                    <destination>..\..\build\install_files</destination>
                    <origin>${AUTOUPDATE_EXE}</origin>
                </copyFile>
            </actionList>
        </actionGroup>
    </preBuildActionList>
    <postInstallationActionList>
        <addDirectoryToPath>
            <insertAt>end</insertAt>
            <path>${installdir}</path>
        </addDirectoryToPath>
        <writeFile>
            <path>${installdir}/addmaidsafeuser.sh</path>
            <text>#!/bin/bash
if  grep -v maidsafe /etc/passwd ; then /usr/sbin/adduser --system --shell /bin/false --no-create-home maidsafe; fi ;  if [ -e /var/cache/maidsafe/ ];then   mkdir -p /var/cache/maidsafe ;  /bin/chown maidsafe.maidsafe /var/cache/maidsafe;fi
; /bin/chmod 755 /etc/init.d/vault</text>
            <ruleList>
                <platformTest>
                    <type>linux</type>
                </platformTest>
            </ruleList>
        </writeFile>
        <changePermissions>
            <files>${installdir}/addmaidsafeuser.sh</files>
            <permissions>+x</permissions>
            <ruleList>
                <platformTest>
                    <type>linux</type>
                </platformTest>
            </ruleList>
        </changePermissions>
        <runConsoleProgram>
            <program>${installdir}/addmaidsafeuser.sh</program>
            <programArguments></programArguments>
            <ruleList>
                <platformTest>
                    <type>linux</type>
                </platformTest>
            </ruleList>
        </runConsoleProgram>
    </postInstallationActionList>
    <allowComponentSelection>1</allowComponentSelection>
    <description>Serverless cloud file storage</description>
    <enableRollback>1</enableRollback>
    <enableTimestamp>1</enableTimestamp>
    <nativePackageName>maidsafePD</nativePackageName>
    <rebootRequired>1</rebootRequired>
    <registerWithPackageDatabase>1</registerWithPackageDatabase>
    <release>1</release>
    <requireInstallationByRootUser>1</requireInstallationByRootUser>
    <saveRelativePaths>1</saveRelativePaths>
    <summary>Serverless cloud file storage</summary>
    <vendor>maidsafe.net</vendor>
    <componentList>
        <component>
  <!-- client -->
